<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            Title="Kevad"
             x:Class="TabbedGorosko.Kevad">
  <!--Pages can be added as references or inline-->
  <ContentPage Title="Märts" >
        <StackLayout>
            <Label Text="Märtsi pilti" FontSize="10"/>
            <Image Source="marts.jpg"/>
            <Button Text="Märts pea pidu" Clicked="Button_Clicked9"/>
        </StackLayout>
    </ContentPage>
    <ContentPage Title="Aprill">
        <StackLayout>
            <Label Text="Aprilli pilti" FontSize="10"/>
            <Image Source="kevadaprill.jpg"/>
            <Button Text="Aprilli pea pidu" Clicked="Button_Clicked10"/>
        </StackLayout>
    </ContentPage>
  <ContentPage Title="Mai">
        <StackLayout>
            <Label Text="Mai pilti" FontSize="10"/>
            <Image Source="kevadmai.jpg"/>
            <Button Text="Mai pea pidu" Clicked="Button_Clicked11"/>
        </StackLayout>
    </ContentPage>
</TabbedPage>